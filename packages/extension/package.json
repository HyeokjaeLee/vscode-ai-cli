{
  "name": "ai-cli",
  "version": "1.0.0",
  "description": "",
  "main": "dist/extension.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode production && cp ../webview-ui/dist/webview.js dist/webview.js",
    "watch": "pnpm --filter webview-ui watch & webpack --watch --mode development"
  },
  "keywords": [],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ai-cli-sidebar-view",
          "title": "AI CLI",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "ai-cli-sidebar-view": [
        {
          "id": "ai-cli-sidebar",
          "name": "AI CLI"
        }
      ]
    },
    "commands": [
      {
        "command": "ai-cli.refresh",
        "title": "Refresh AI CLI"
      }
    ]
  },
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@types/vscode": "*",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
}
